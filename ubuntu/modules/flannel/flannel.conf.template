[program:flannel]
command=/opt/flannel/flanneld -etcd-endpoints=%(endpoint)s
directory=/opt/flannel
autostart=true
autorestart=true
startsecs=10
stopwaitsecs=10
redirect_stderr=true
stdout_logfile=/var/log/flannel/flannel.log
stdout_logfile_maxbytes=1MB
stdout_logfile_backups=10
stdout_capture_maxbytes=1MB
